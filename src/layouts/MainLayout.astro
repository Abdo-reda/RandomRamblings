---
import IndexLayout from "./IndexLayout.astro";
import Header from "../components/Header.astro";
import Footer from "../components/Footer.astro";
const { pageTitle } = Astro.props;
---

<IndexLayout pageTitle={pageTitle}>
  <Header />
  <main>
    <div class="content">
      <slot />
    </div>
    <Footer />
  </main>
</IndexLayout>
